{
  "extends": [
    "@pob/root/tsconfigs/strict.json",
    "@pob/root/tsconfigs/targets/rollup-es2015.json"
  ],
  "include": ["lib/phosphor-icons.mjs", "lib/phosphor-icons.d.ts"],
  "compilerOptions": {
    "rootDir": "lib",
    "outDir": "dist/definitions",
    "tsBuildInfoFile": "node_modules/.cache/tsc/tsbuildinfo",
    "noEmitOnError": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": true,
    "emitDeclarationOnly": false,
    "incremental": true,
    "composite": true
  }
}
