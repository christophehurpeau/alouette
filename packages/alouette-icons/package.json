{
  "name": "alouette-icons",
  "version": "8.0.1",
  "description": "icons for alouette",
  "keywords": [],
  "author": "Christophe Hurpeau <302891+christophehurpeau@users.noreply.github.com> (https://christophe.hurpeau.com)",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/christophehurpeau/alouette.git",
    "directory": "packages/alouette-icons"
  },
  "homepage": "https://github.com/christophehurpeau/alouette",
  "type": "module",
  "engines": {
    "node": ">=22.21.1"
  },
  "sideEffects": false,
  "react-native": {
    "./phosphor-icons": "./lib/phosphor-icons.cjs"
  },
  "exports": {
    "./package.json": "./package.json",
    "./phosphor-icons/*": {
      "types": "./lib/phosphor-icons/*.d.ts",
      "react-native": {
        "import": "./lib/phosphor-icons/*.react-native.mjs"
      },
      "browser": "./lib/phosphor-icons/*.web.mjs",
      "web": "./lib/phosphor-icons/*.web.mjs",
      "node": {
        "web": {
          "import": "./lib/phosphor-icons/*.web.mjs"
        },
        "react-native": {
          "import": "./lib/phosphor-icons/*.react-native.mjs"
        },
        "import": "./lib/phosphor-icons/*.web.mjs"
      }
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn run build:definitions",
    "build:definitions": "tsc -p tsconfig.json",
    "lint": "yarn run lint:eslint",
    "lint:eslint": "yarn ../.. run eslint --quiet packages/alouette-icons",
    "test": "yarn ../../ run test -- packages/alouette-icons"
  },
  "pob": {
    "entries": [
      "phosphor-icons"
    ],
    "typescript": "check-only"
  },
  "prettier": "@pob/root/prettier-config",
  "peerDependencies": {
    "react": "^19.1.0",
    "react-native": "*",
    "react-native-svg": "*"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@phosphor-icons/core": "2.1.1",
    "react-native": "0.81.4",
    "typescript": "5.9.3"
  }
}
